-- Function to retrieve the SteamID64 (Hex) value from a memory record
function getSteamID64()
    local memoryRecords = getAddressList()

    -- Replace with the correct memory record description
    local targetDescription = "SteamID64 (Hex) 003"

    for i = 0, memoryRecords.Count - 1 do
        local memrec = memoryRecords[i]
        if memrec.Description == targetDescription then
            return memrec.Value
        end
    end

    showMessage("Error: Memory record not found")
    return nil
end

-- Function to construct the web address and copy it to the clipboard
function copySteamProfileLink()
    local HexID = getSteamID64()
    if HexID == nil then
        return
    end
    local WebAddress = "https://steamcommunity.com/profiles/" .. tostring(tonumber(HexID, 16))
    writeToClipboard(WebAddress)
end

-- Call the function to copy the Steam profile link
copySteamProfileLink()
autoDisable(memrec)
