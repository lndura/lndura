function removeEffect(input)
  local pre = readQword("[[[WorldChrMan]+80]+18]+18")
  local ptr = "[[[[WorldChrMan]+80]+18]+18]+30"
  if type(input) == "number" then
    writeInteger(ptr, input)
    executeCodeEx(0,-1,"DarkSoulsIII.exe+9FDFB0", pre, 3)
  elseif type(input) == "table" then
    for _,i in ipairs(input) do
      createThread(function()
        writeInteger(ptr, i)
        executeCodeEx(0,-1,"DarkSoulsIII.exe+9FDFB0", pre,3)
        sleep(100)
      end)
    end
  end
end

