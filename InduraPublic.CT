		local id = 6020
		for i=0,bonfireLv-1 do
			SetEventState(id+i, 1)
		end
		for i=bonfireLv,maxBonfireLv-1 do
			SetEventState(id+i, 0)
		end
	end
end

local function SetBonfireLv(bonfireLv)
	if bonfireLv &gt;= 11 then
