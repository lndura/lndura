label(code)
label(advanceState)
label(popandreturn)

newmem:

//changed movaps -&gt; movaps

code:
  movaps xmm1,[sfxrgbasm] //mov base //movaps
  inc [sfxcounter]
  pushf
  push rax
  push rdx
  xor eax,eax
  xor edx,edx
  mov eax,[sfxrgbcountermax] //test for counter max
  cvttss2si edx, [sfxrgb_speed]
  mov [converted_mult],edx
  mul rdx
  mov [actual_sfxrgbcountermax],eax
  pop rdx
  cmp [sfxcounter],eax
  jge advanceState
  cmp [colorstateasm], 0
  je sfxst0
  cmp [colorstateasm], 1
  je sfxst1
  cmp [colorstateasm], 2
  je sfxst2
  cmp [colorstateasm], 3
  je sfxst3
  cmp [colorstateasm], 4
  je sfxst4
  cmp [colorstateasm], 5
  je sfxst5
  pop rax //default shouldnt be reached
  popf
  jmp return

sfxst0:
  pop rax
  popf
  movaps [tmp_float], xmm0 //movaps

  movaps xmm0,[sfxaddgreen] //movaps

  divps xmm0, [sfxrgb_speed]
  divps xmm0,[sfxrgb_intensity]
  addps xmm1,xmm0
  movaps [sfxrgbasm],xmm1 //refresh //movaps
  movaps xmm0,[tmp_float] //movaps
  jmp return

sfxst1:
  pop rax
  popf
  movaps [tmp_float], xmm0 //movaps

  movaps xmm0,[sfxaddred] //movaps

  divps xmm0, [sfxrgb_speed]
  divps xmm0,[sfxrgb_intensity]
  subps xmm1,xmm0
  movaps [sfxrgbasm],xmm1 //refresh //movaps
  movaps xmm0,[tmp_float] //movaps
  jmp return

sfxst2:
  pop rax
  popf
  movaps [tmp_float], xmm0 //movaps

  movaps xmm0,[sfxaddblue] //movaps

  divps xmm0, [sfxrgb_speed]
  divps xmm0,[sfxrgb_intensity]
  addps xmm1,xmm0
  movaps [sfxrgbasm],xmm1 //refresh //movaps
  movaps xmm0,[tmp_float] //movaps
  jmp return

sfxst3:
  pop rax
  popf
  movaps [tmp_float], xmm0 //movaps

  movaps xmm0,[sfxaddgreen] //movaps

  divps xmm0, [sfxrgb_speed]
  divps xmm0,[sfxrgb_intensity]
  subps xmm1,xmm0
  movaps [sfxrgbasm],xmm1 //refresh //movaps
  movaps xmm0,[tmp_float] //movaps
  jmp return

sfxst4:
  pop rax
  popf
  movaps [tmp_float], xmm0 //movaps

  movaps xmm0,[sfxaddred] //movaps

  divps xmm0, [sfxrgb_speed]
  divps xmm0,[sfxrgb_intensity]
  addps xmm1,xmm0
  movaps [sfxrgbasm],xmm1 //refresh //movaps
  movaps xmm0,[tmp_float] //movaps
  jmp return

sfxst5:
  pop rax
  popf
  movaps [tmp_float], xmm0 //movaps

  movaps xmm0,[sfxaddblue] //movaps

  divps xmm0, [sfxrgb_speed]
  divps xmm0,[sfxrgb_intensity]
  subps xmm1,xmm0
  movaps [sfxrgbasm],xmm1 //refresh //movaps
  movaps xmm0,[tmp_float] //movaps
  jmp return

advanceState:
  pop rax
  mov [sfxcounter],0
  inc [colorstateasm]
  cmp [colorstateasm], 6 //maxstates
  jl popandreturn
  popf
  mov [colorstateasm],0
  movaps xmm1, [sfxrgbasmbase] //reset //movaps
  divps xmm1,[sfxrgb_intensity]
  movaps [sfxrgbasm],xmm1 //refresh //movaps
  jmp return

popandreturn:
  popf
  jmp return

TheThing:
  jmp newmem
  nop
  nop
return:

