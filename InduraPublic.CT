	elseif bonfireLv &lt;= 1 then
		bonfireLv = 1
	end
	if GetTotalBonfireLevel() &lt;= 11 then
		ReplaceFlasks(bonfireLv)
		ActivateBonfire(bonfireLv)
		BonfireInjectAnim()
		UpdateBonfireEvent()
		SaveRequest()
	end
end

