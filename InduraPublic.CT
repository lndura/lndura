createThread(function()
  writeFloat(player0xCoordPtr, x)
  writeFloat(player0zCoordPtr, z)
  writeFloat(player0yCoordPtr, y)
  writeFloat(player0AnglePtr, a)
  sleep(50)
  writeInteger(allDeathCamPtr, 0)
end)
else
createThread(function()
  writeBit(noDeadPtr, 2, 1)
  sleep(25)
  writeFloat(player0xCoordPtr, x)
  writeFloat(player0zCoordPtr, z)
  writeFloat(player0yCoordPtr, y)
  writeFloat(player0AnglePtr, a)
  sleep(50)
  writeBit(noDeadPtr, 2, 0)
  writeInteger(allDeathCamPtr, 0)
end)
end end

